import React from 'react';
import { Form, FormGroup, FormSubmit } from 'kit';

const {{.opts.Name.Pascalize}}Form = (props) => {
  return (
    <Form {...props}>
      {{- range .opts.Attrs -}}
        {{- if ne .Name.Camelize.String "ID"  }}
          <FormGroup name="{{ .Name.Camelize }}" label="{{ .Name.Pascalize }}" />
        {{- end }}
      {{- end }}
      <FormSubmit />
    </Form>
  );
};

export default {{.opts.Name.Pascalize}}Form;
