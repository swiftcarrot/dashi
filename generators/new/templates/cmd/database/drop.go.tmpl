package database

import (
  "github.com/spf13/cobra"
  "{{ .opts.Package }}/dashi"
)

var DropCmd = &cobra.Command{
  Use:   "drop",
  Short: "Database drop",
  RunE: func(cmd *cobra.Command, args []string) error {
    s, err := dashi.NewServer()
    if err != nil {
      return err
    }

    return s.DatabaseDrop()
  },
}
