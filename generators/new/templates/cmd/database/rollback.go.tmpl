package database

import (
  "github.com/spf13/cobra"
  "{{ .opts.Package }}/dashi"
)

var RollbackCmd = &cobra.Command{
  Use:   "rollback",
  Short: "Database rollback",
  RunE: func(cmd *cobra.Command, args []string) error {
    s, err := dashi.NewServer()
    if err != nil {
      return err
    }

    return s.DatabaseRollback()
  },
}
